# Base API URL for the frontend
# In development, keep this relative so requests go through the Vite proxy
VITE_API_BASE_URL=/api

# Target API server for the Vite dev proxy (used only in development)
# Change this if you want to point to a different backend while developing
DEV_API_PROXY_TARGET=https://adnanshops.com

# Examples:
# VITE_API_BASE_URL=http://localhost/api
# DEV_API_PROXY_TARGET=http://localhost
